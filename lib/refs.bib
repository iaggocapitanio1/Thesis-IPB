@INPROCEEDINGS{SeungSME,  author={Son, Seung-Beom and Kwon, Jun-Yeong and Kim, Chae-Soo},  booktitle={2019 IEEE International Conference on Big Data (Big Data)},   title={A Study on Solution Oriented Smart Factory Diagnostic System for SME},   year={2019},  volume={},  number={},  pages={6101-6103},  abstract={This paper is a case study result of developing a system for the diagnosis of smart factories suitable for the situation of Korean SMEs, and it is characterized by the composition of the diagnostic items centered on smart factor element technology and the generation of design reports based on the diagnosis results. The diagnostic item of this system is composed of the minimum items centered on the element technology and the representative functions of each solution. Therefore, the diagnostic time is usually less than 4 hours. The diagnostic report consists of the level of work area and construction rate by element technologies, and the report includes the road map for each step. As a result of applying this system to the actual site, the satisfaction of the person who performed the diagnosis and the company responding to the diagnosis showed relatively high satisfaction compared to the existing diagnosis method.},  keywords={},  doi={10.1109/BigData47090.2019.9006147},  ISSN={},  month={Dec},}

